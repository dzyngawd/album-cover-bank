---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';
---

<Layout title="Welcome to Astro.">
	<main>
		<body class="body">
    <div data-animation="default" data-collapse="small" data-duration="400" data-easing="ease" data-easing2="ease"
        role="banner" class="header w-nav"><a href="/" aria-current="page" class="brand w-nav-brand w--current"><img
                src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/6383fe96d7b9aeba745eab20_logo%20(1).png"
                loading="lazy" width="57" alt="" class="logo-image" /></a>
        <div class="w-nav-button">
            <div class="icon w-icon-nav-menu"></div>
        </div>
        <nav role="navigation" class="menu-link w-nav-menu"><a href="/database.html" class="nav-link w-nav-link">Cover
                Bank</a><a href="/stories.html" class="nav-link w-nav-link">Stories</a><a href="/about.html"
                class="nav-link w-nav-link">About</a></nav>
    </div>
    <div class="hero-section wf-section">
        <div class="w-layout-grid grid-2">
            <div id="w-node-ecffeffa-0248-6d76-aa03-1d07d5f9be6c-711b079f" class="left-column">
                <div class="home-header-text">
                    <div class="header-text">A Retrospective of Album Cover Art</div>
                </div>
                <div class="home-subtittle-text">
                    <div class="subtitle-text">The album cover is one of the most important and enduring features of
                        contemporary visual culture. This project explores the history of album covers from 1960 to
                        2022.</div>
                </div><a href="#" class="home-button w-button">View Cover Bank</a>
            </div>
            <div class="right-column">
                <div class="hightlight-background">
                    <div class="spotlight">
                        <div class="spotlight-tittle">
                            <div class="text-spotlight-tmain">SPOTLIGHT</div>
                        </div>
                        <div class="spotlight-date">
                            <div class="text-spotlight-tdate">TODAY</div>
                        </div>
                    </div>
                    <div class="featured-spotlight">
                        <div class="spotlight-image-cover"><img
                                src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/637efddde2694137e7d6687e_Rectangle%201.png"
                                loading="lazy" width="230" height="230" alt="" class="image" /></div>
                        <div class="spotlight-album-details">
                            <div class="cover-info">
                                <div class="tittle">
                                    <div class="cover-info-tittle">Beasts of No Nation</div>
                                </div>
                                <div class="subtittle">
                                    <div class="genre">
                                        <div class="label-genre">
                                            <div class="text-label-genre">GENRE</div>
                                        </div>
                                        <div class="tag-genre">
                                            <div class="text-tag-genre">AfroBeat</div>
                                        </div>
                                    </div>
                                    <div class="artiste">
                                        <div class="label-artiste">
                                            <div class="text-label-artiste">ARTIST</div>
                                        </div>
                                        <div class="tag-artiste">
                                            <div class="text-tag-artiste">Fela Anikulapo Kuti &amp; Egypt 80</div>
                                        </div>
                                    </div>
                                    <div class="year">
                                        <div class="label-year">
                                            <div class="text-label-year">YEAR</div>
                                        </div>
                                        <div class="tag-artiste">
                                            <div class="text-tag-year">1989</div>
                                        </div>
                                    </div>
                                    <div class="designer">
                                        <div class="label-designer">
                                            <div class="text-label-designer">DESIGNER</div>
                                        </div>
                                        <div class="tag-designer">
                                            <div class="text-tag-designer">Ghariokwu Lemi Arts Company</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="user-button"><a href="#" class="like-button-web w-button">Like</a><a href="#"
                                    class="share-button-web w-button">Share</a></div>
                        </div>
                    </div>
                    <div class="previous-section">
                        <div class="previous-header">
                            <div>PREVIOUS</div>
                        </div>
                        <div class="previous-image-container">
                            <div class="previouse-image-web">
                                <div data-w-id="2372317c-ac0c-5026-b5bc-27f6c40d493a" class="overlay"></div><img
                                    src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/638080594ccc8f2ea3b97ee1_previous-image-web%20(1).png"
                                    loading="lazy" alt="" class="previous-image" />
                            </div>
                            <div class="previouse-image-web">
                                <div data-w-id="2372317c-ac0c-5026-b5bc-27f6c40d493d" class="overlay"></div><img
                                    src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/638081873df186729f915d02_previous-image-web%20(2).png"
                                    loading="lazy" alt="" class="previous-image" />
                            </div>
                            <div class="previouse-image-web">
                                <div data-w-id="2372317c-ac0c-5026-b5bc-27f6c40d4940" class="overlay"></div><img
                                    src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/63808277f0ef344404227a35_previous-image-web.png"
                                    loading="lazy" alt="" class="previous-image" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="footer wf-section">
        <div class="footer-break-line"></div>
        <div class="footer-details">
            <div class="footer-text">Â© wuruwuru studio. All Rights Reserved</div>
            <div class="footer-privacy">
                <div class="footer-text">Privacy Policy</div>
                <div class="footer-text">Terms and Conditions</div>
            </div>
        </div>
    </div>
    <script src="./script.js"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="https://uploads-ssl.webflow.com/6328a96b9437f7a3a21b079e/js/webflow.df636265a.js"
        type="text/javascript"></script>
    <!--[if lte IE 9]><script src="//cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif]-->
</body>
	</main>
</Layout>


